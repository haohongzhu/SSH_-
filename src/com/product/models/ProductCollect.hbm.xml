<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.product.models.ProductCollect" table="Table_Collect">
		<id name="id" column="id" >
			<generator class="native"/>
		</id>
		<many-to-one name="customerID4" class="com.customer.models.Customer" column="customerID" lazy="false"></many-to-one>
		<many-to-one name="productID3" class="com.product.models.Product" column="productID" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>
